<% provide(:title, 'Create Subject') %>
<h1>Create Subject</h1>
<div class="row">
  <div class="span6 offset3">
   
    <%= form_for @quota, url: createQuota_path(@quota) , html: {method: "post"} do |f|  %>

    <%= p_fieldset('default',legend: 'Datos BÃ¡sicos',style: 'margin-bottom:10px') do %>      
      <%= f.hidden_field :id  %>  
      <%= f.label :subject_id %>
