<li>
  <span class="content"><%= program.name %></span>
  <span class="content"><%= program.code %></span>
  <%if @user.usertype.eql?("Estudiante")%>
    <% if !@user.subject_records.any? && !@user.preregister_subjects.any? %>
      | <%= link_to I18n.t('programas.autoregistro'), preregister_subject_path(:automatic => 1,:user_id =>@user.id,:program_id =>program.id), method: :patch,
        data: { confirm: I18n.t('seguro')} %>
    <% end %>
    <% if  @user.preregister_subjects.count < 10 %>
      | <%= link_to "New Preregister Subject", new_preregister_subject_path(:user_id =>@user.id,:program_id =>program.id), method: :get %>
    <% end %>
  <% end %>
</li>