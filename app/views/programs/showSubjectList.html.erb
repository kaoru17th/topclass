<% provide(:title, 'Todas las materias por programa') %>

<h1>Todas las materias por programa</h1>

<ul class="subjects">
  <table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>ID Materia</th>
      <th>Nombre</th>
      <th>Quota</th>
      <th>Preinscripci√≥n</th>
      <th>Porcentaje</th>
    </tr>
  </thead>
  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.idSubject %></td>
        <td><%= subject.codeSubject %></td>
        <td><%= subject.subjectName %></td>
        <td><%= subject.subjectQuota %></td>
