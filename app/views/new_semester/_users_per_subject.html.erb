<div id="users_per_subject">
	<table class="table table-hover">
		<thead>
			<th>#</th>
			<th>Nombre</th>
			<th>Apellido</th>
			<th>Codigo</th>
		</thead>
		<tbody>
			<tr>
				<%@users_per_subject.each do |user_subject|%>
					<td><%=user_subject.id %></td>
					<td><%=user_subject.firstname%></td>
					<td><%=user_subject.lastname%></td>
					<td><%=user_subject.identificationtype%></td>
				<% end %>
			</tr>
		</tbody>
	</table>
</div>